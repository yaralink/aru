          "_name": "alist2",
          "_sourceTheme": "platformm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"list2 platformm5\" group=\"List\" data-bg-video=\"{{bg.type == 'video' &amp;&amp; bg.value.url}}\" mbr-class=\"{'mbr-parallax-background':bg.parallax}\" plugins=\"mbr-switch-arrow, mobirise-icons2\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" name=\"textWidth\" title=\"Width\" min=\"4\" max=\"8\" value=\"8\" step=\"1\">\n        <input type=\"checkbox\" name=\"reverseContent\" title=\"Reverse\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title Marker\" name=\"showTitleMarker\" checked=\"\" condition=\"showTitle\">\n        <input type=\"color\" title=\"Marker Color\" name=\"markerColor\" value=\"#AAFC75\" condition=\"showTitle &amp;&amp; showTitleMarker\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"BG Frame\" name=\"showFrame\" checked=\"\">\n        <input type=\"color\" title=\"BG Frame Color\" name=\"frameColor\" value=\"#AAFC75\" condition=\"showFrame\">\n        <input type=\"range\" inline=\"\" name=\"frameOpacity\" title=\"BG Frame Opacity\" min=\"0.1\" max=\"1\" value=\"0.7\" step=\"0.1\" condition=\"showFrame\">\n        <input type=\"range\" inline=\"\" name=\"frameSize\" title=\"BG Frame Size\" min=\"1\" max=\"100\" value=\"30\" step=\"1\" condition=\"showFrame\">\n        <header>List</header>\n        <input type=\"color\" title=\"List Hover Color\" name=\"listHoverColor\" value=\"#AAFC75\">\n        <input type=\"checkbox\" title=\"Border\" name=\"showListBorder\" checked=\"\">\n        <input type=\"color\" title=\"Border Color\" name=\"listBorderColor\" value=\"#FFFFFF\" condition=\"showListBorder\">\n        <input type=\"range\" name=\"borderOpacity\" title=\"Border Opacity\" min=\"0.1\" max=\"1\" step=\"0.1\" value=\"0.2\" condition=\"showListBorder\">\n        <input type=\"checkbox\" title=\"Icon\" name=\"showListIcon\" checked=\"\">\n        <input type=\"color\" title=\"Icon Color\" name=\"listIconColor\" value=\"#AAFC75\" condition=\"showListIcon\">\n        <select title=\"Amount\" name=\"cardAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\" selected=\"\">3</option>\n            <option value=\"4\">4</option>\n        </select>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax=\"\">\n            <input type=\"image\" title=\"Image\" value=\"../_images/background3.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#173D33\" selected=\"\">\n            <input type=\"video\" title=\"Video\" value=\"https://vimeo.com/428046504\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"../_images/background3.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked=\"\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#173D33\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.9\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <div class=\"bg-frame\" mbr-if=\"showFrame\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        \n            <div class=\"row justify-content-center\">\n                <div class=\"col-12 col-md col-title\" mbr-if=\"showTitle || showSubtitle\">\n                    <h5 class=\"mbr-section-title mbr-fonts-style\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-7\">\n                        OUR STRONG POINTS\n                    </h5>\n                    <h6 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-7\">\n                        Diam quam nulla porttitor massa id neque.Cursus turpis massa tincidunt dui ut ornare\n                        lectus sit amet.\n                    </h6>\n                </div>\n\n                <div class=\"col-md-12 col-lg-{{textWidth}}\">\n                    <div id=\"bootstrap-toggle\" class=\"toggle-panel accordionStyles tab-content\">\n                        <div class=\"card mb-0\">\n                            <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                <a role=\"button\" class=\"collapsed panel-title text-black\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core=\"\" href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n                                    <h6 class=\"panel-title-edit mbr-fonts-style mb-0\" mbr-theme-style=\"display-1\" data-app-selector=\".panel-title-edit\">\n                                        Our goals\n                                    </h6>\n                                    <span class=\"sign mbr-iconfont mbri-arrow-d