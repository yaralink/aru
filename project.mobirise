          "_sourceTheme": "globalm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"footer1\" group=\"Footers\" global once=\"footers\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\">\n\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Main\" name=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Logo\" name=\"showLogo\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"List Title\" name=\"showListTitle\" condition=\"showMain\">\n        <input type=\"checkbox\" title=\"Contacts\" name=\"showContacts\" condition=\"showMain\" checked>\n        <input type=\"checkbox\" title=\"Social\" name=\"showSocial\" condition=\"showMain\" checked>\n        <input type=\"range\" name=\"socialCount\" title=\"Social Count\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showMain\">\n        <input type=\"checkbox\" title=\"Copyright\" name=\"showCopyright\" checked>\n        <input type=\"checkbox\" title=\"Copyright List\" name=\"showCopyrightList\" condition=\"showCopyright\" checked>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"color\" title=\"Color\" value=\"#102d49\" selected>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/globalm5/components/_images/background1.jpg\">\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/globalm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#124AD6\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row row-main\" mbr-if=\"showMain\">\n            <div class=\"col-12 col-lg-4 title\">\n                <div class=\"title-wrapper\">\n                    <img src=\"@PROJECT_PATH@/assets/images/2-80x80.png\" alt=\"Mobirise\" mbr-if=\"showLogo\">\n                    <h2 mbr-text class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title, .mbr-iconfont\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\">\n                        <b>Aru Namgyal</b></h2>\n                </div>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-3 card-list\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showListTitle\">\n                    <b>Help</b>\n                </h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-5\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\"><b>Help</b></li>\n                    <li class=\"mbr-text item-wrap\"><b>Forums</b></li>\n                    <li class=\"mbr-text item-wrap\"><b>Global Kit</b></li>\n                </ul>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-3 card-list\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showListTitle\">\n                    <b>Projects</b>\n                </h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\">General project</li>\n                    <li class=\"mbr-text item-wrap\">Control test</li>\n                </ul>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-3 card-list\">\n                <h5 mbr-text class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-4\" mbr-if=\"showListTitle\">\n                    <b>Services</b>\n                </h5>\n                <ul class=\"list mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list\" data-multiline mbr-article>\n                    <li class=\"mbr-text item-wrap\">Engineering</li>\n                    <li class=\"mbr-text item-wrap\">Labour hire</li>\n                    <li class=\"mbr-text item-wrap\">Programmed plant</li>\n                </ul>\n            </div>\n            <div class=\"col-12 col-lg-2 col-md-3\">\n                <h5 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle, .social-wrapper\" mbr-theme-style=\"display-4\" mbr-if=\"showListTitle\">\n                    <b>About us</b>\n                </h5>\n                <p class=\"mbr-contact mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-contact\" mbr-if=\"showContacts\">\n                    (123)456-7890</p>\n                <p class=\"mbr-contact mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-contact\" mbr-if=\"showContacts\">\n                    mobi@rise.com</p>\n                <div class=\"social-wrapper\">\n                    <div class=\"social-row display-7\" mbr-if=\"showSocial\">\n                        <div class=\"soc-item\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-facebook\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"socialCount>1\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-twitter\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"socialCount>2\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-instagram\"></span>\n                            </a>\n                        </div>\n                        <div class=\"soc-item\" mbr-if=\"socialCount>3\">\n                            <a href=\"https://twitter.com/mobirise\" target=\"_blank\">\n                                <span mbr-icon class=\"mbr-iconfont socicon socicon-instagram\"></span>\n                            </a>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n        <div class=\"row\" mbr-if=\"showCopyright\">\n            <div class=\"col-12 copyright-container\">\n                <div class=\"copyright-wrapper\">\n                    <p class=\"mbr-fonts-style copyright\" mbr-theme-style=\"display-7\" data-app-selector=\".copyright, .copyright-list\" data-app-placeholder=\"Type Text\">\n                         Copyright 2022 Mobirise - All Rights Reserved\n                    </p>\n                </div>\n                <div class=\"copyright-list\" mbr-if=\"showCopyrightList\">\n                    <ul class=\"list list-copy mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".list-copy\" data-multiline mbr-article>\n                        <li class=\"mbr-copy item-wrap\">Privacy Police</li>\n                        <li class=\"mbr-copy item-wrap\">Terms of Use</li>\n                        <li class=\"mbr-copy item-wrap\">Sitemap</li>\n                    </ul>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
          "_styles": {
            "padding-top": "(@paddingTop * 1rem)",
            "padding-bottom": "(@paddingBottom * 1rem)",
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)"
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            ".mbr-fallback-image.disabled": {
              "display": "none"
            },
            ".mbr-fallback-image": {
              "display": "block",
              "background-size": "cover",
              "background-position": "center center",
              "width": "100%",
              "height": "100%",
              "position": "absolute",
              "top": "0",
              "& when (@bg-type = 'video')": {
                "background-image": "url(@fallBackImage)"
              }
            },
            ".row-main": {
              "margin-bottom": "200px",
              "@media (max-width: 992px)": {
                "margin-bottom": "140px"
              },
              "@media (max-width: 768px)": {
                "margin-bottom": "60px"
              }
            },
            ".title": {
              "@media (max-width: 992px)": {
                "display": "none"
              }
            },
            ".title-wrapper": {
              "display": "flex",
              "img": {
                "width": "40px",
                "height": "40px",
                "object-fit": "cover",
                "margin-right": "12px",
                "border-radius": "15%"
              },
              ".mbr-section-title": {
                "margin": "0"
              }
            },
            ".list": {
              "list-style": "none",
              "padding-left": "0",
              "@media (max-width: 991px)": {
                "margin-bottom": "2rem"
              },
              "@media (min-width: 992px)": {
                "margin-bottom": "0"
              },
              "color": "#ffffff"
            },
            ".social-wrapper": {
              "width": "100%"
            },
            ".social-row": {
              "display": "inline-flex",
              "flex-wrap": "wrap",
              "margin-top": "20px",
              "@media (max-width: 992px)": {
                "margin-top": "12px"
              }
            },
            ".soc-item": {
              "display": "flex",
              "align-items": "center",
              "justify-content": "center",
              "margin-right": "18px",
              "margin-bottom": "20px",
              "height": "18px",
              "width": "18px",
              "a span": {
                "font-size": "18px"
              }
            },
            ".card-list": {
              "@media (max-width: 768px)": {
                "display": "none"
              }
            },
            ".mbr-section-subtitle": {
              "margin-bottom": "20px",
              "@media (max-width: 992px)": {
                "margin-bottom": "12px"
              },
              "@media (max-width: 768px)": {
                "display": "none"
              },
              "color": "#8c8c8c"
            },
            ".mbr-text": {
              "margin-bottom": "20px",
              "transition": "all .3s ease",
              "@media (max-width: 992px)": {
                "margin-bottom": "12px"
              }
            },
            ".mbr-contact": {
              "margin-bottom": "0",
              "padding-bottom": "6px",
              "transition": "all .3s ease",
              "@media (max-width: 992px)": {
                "padding-bottom": "4px"
              },
              "color": "#ffffff"
            },
            ".copyright-container": {
              "display": "flex",
              "justify-content": "center",
              "@media (max-width: 768px)": {
                "display": "block"
              },
              ".copyright-wrapper": {
                "width": "50%",
                "@media (max-width: 768px)": {
                  "width": "100%"
                },
                ".copyright": {
                  "margin-bottom": "20px",
                  "transition": "all .3s ease"
                }
              },
              ".copyright-list": {
                "width": "50%",
                "@media (max-width: 768px)": {
                  "width": "100%"
                },
                "ul": {
                  "display": "inline-flex",
                  ".mbr-copy": {
                    "margin": "0 30px 0 0",
                    "transition": "all .3s ease"
                  }
                }
              }
            },
            ".item-wrap:hover, p:hover, .mbr-iconfont:hover": {
              "color": "@primaryColor"
            },
            ".mbr-section-title, .mbr-iconfont": {
              "color": "#ffffff"
            },
            ".copyright": {
              "color": "#000000"
            },
            ".list-copy": {
              "color": "#ffffff"
            },
            ".copyright, .copyright-list": {
              "color": "#ffffff"
            }
          },
          "_cid": "tFygJXIgR9",
          "_protectedParams": [],
          "_global": true,
          "_once": "footers",
          "_params": {},
          "_anchor": "footer1-3"
        }
      ]
    }
  }
